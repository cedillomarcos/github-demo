<com.rlk.scene.CellLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:launcher="http://schemas.android.com/apk/res/com.rlk.scene"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    launcher:cellWidth="@dimen/cell_width" 
    launcher:cellHeight="@dimen/cell_height" 
    launcher:longAxisStartPadding="@dimen/long_axis_start_Padding"
    launcher:longAxisEndPadding="@dimen/long_axis_end_Padding"
    launcher:shortAxisStartPadding="@dimen/short_axis_start_Padding"
    launcher:shortAxisEndPadding="@dimen/short_axis_end_Padding"
    launcher:shortAxisCells="@integer/short_Axis_Cells"
    launcher:longAxisCells="@integer/long_Axis_Cells" />
